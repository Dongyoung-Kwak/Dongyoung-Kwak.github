<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ipad.project.getRegionData.dao.IGetRegionData">
	<select id="getRegionCode" parameterType="String" resultType="string">
		select adm_cd from region where region_name_detail = #{adm_cd}
	</select>
	
	<select id="getVariableSale" parameterType="String" resultType="int">
		select twenties, thirties, sixties, over70s, floatPp, income, dentalClinic, subway from region_data where adm_cd=#{adm_cd}
	</select>
	
	<resultMap id="getWeightPoint" type="com.ipad.project.getRegionData.model.PointVO">
		<result property="twentiesPoint" column="POPULATION_20_POINT"/>
		<result property="thirtiesPoint" column="POPULATION_30_POINT"/>
		<result property="sixtiesPoint" column="POPULATION_60_POINT"/>
		<result property="over70sPoint" column="POPULATION_OVER70_POINT"/>
		<result property="floatPoint" column="FLOATPP_POINT"/>
		<result property="incomePoint" column="INCOME_POINT"/>
		<result property="dentalClinicPoint" column="DENTALCLINIC_POINT"/>
		<result property="subwayPoint" column="SUBWAY_POINT"/>
	</resultMap>
</mapper>	
	